<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="RedContactos.View.AltaView">
  <StackLayout>
    <Label Text="Login"/>
    <Entry Text="{Binding Usuario.login}"/>
    <Label Text="Password"/>
    <Entry Text="{Binding Usuario.password}"/>
    <Label Text="Nombre"/>
    <Entry Text="{Binding Usuario.nombre}"/>
    <Label Text="Apellidos"/>
    <Entry Text="{Binding Usuario.apellidos}"/>
    <Button Text="Nuevo usuario" Command="{Binding CmdAlta}"/>
  </StackLayout>
</ContentPage>